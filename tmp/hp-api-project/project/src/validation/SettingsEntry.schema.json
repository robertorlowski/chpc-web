{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SettingsEntry",
  "type": "object",
  "properties": {
    "night_hour": {
      "$ref": "#/$defs/TimeSlot"
    },
    "settings": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/TimeSlot"
      }
    },
    "cwu_settings": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/TimeSlot"
      }
    }
  },
  "additionalProperties": false,
  "$defs": {
    "TimeSlot": {
      "type": "object",
      "properties": {
        "slot_start_hour": {
          "type": "number"
        },
        "slot_start_minute": {
          "type": "number"
        },
        "slot_stop_hour": {
          "type": "number"
        },
        "slot_stop_minute": {
          "type": "number"
        }
      },
      "additionalProperties": false
    }
  }
}